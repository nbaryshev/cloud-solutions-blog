          </li>
            {% if current_user.is_anonymous %}
            <li class="nav-item">
            <a class="nav-link" href="{{ url_for('signup') }}">Sign Up</a>
          </li>
            <li class="nav-item">
            <a class="nav-link" href="{{ url_for('signin') }}">Sign In</a>
          </li>
            {% else %}
             <li class="nav-item">
            <a class="nav-link" href="{{ url_for('signout') }}">Sign Out</a>
          </li>
            {% endif %}
        </ul>
      </div>
    </div>
  </nav>

