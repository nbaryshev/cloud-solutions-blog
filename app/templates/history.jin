{% extends 'base.jin' %}

{% block main %}
{% for post in posts %}
<div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
    <p><h2><a href="{{ url_for('post', post_id=post.post_id) }}">{{ post.heading }}</a></h2></p>
    <p>{{ post.post_text}}</p>
    <p>The post was added at: {{ post.post_time }}</p>
    </div>
    <div class="col-md-3"></div>
</div>
{% endfor %}
{% endblock %}