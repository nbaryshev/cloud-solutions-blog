{% extends 'base.jin' %}

{% block main %}

<form action="" method="POST">
    {{ form.hidden_tag() }}
    <p>
    {{ form.email.label }}
        {{ form.email() }}
    </p>
    <p>
    {{ form.pwd.label }}
        {{ form.pwd() }}
    </p>
    <p>
        {{ form.remember.label }}
        {{ form.remember() }}
    </p>
    <p>
        {{ form.submit() }}
    </p>
</form>

{% endblock %}